<ul id="share">
  <li class="item">
    <a href="http://twitter.com/share" class="twitter-share-button" data-count="vertical" data-url="<%= HOST %>" data-text="Keep track of all your TV shows:" data-via="episodecalendar.com" data-related="frexuz">Tweet</a><script type="text/javascript" src="http://platform.twitter.com/widgets.js"></script>
  </li>
  <li class="item">    
    <!-- Place this tag where you want the +1 button to render -->
    <g:plusone size="tall" href="<%= HOST %>"></g:plusone>
    
    <!-- Place this render call where appropriate -->
    <script type="text/javascript">
      (function() {
        var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
        po.src = 'https://apis.google.com/js/plusone.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
      })();
    </script>
  </li>
  <li class="item">
    <iframe src="http://www.facebook.com/plugins/like.php?app_id=236740686371391&amp;href=<%= HOST %>&amp;send=false&amp;layout=box_count&amp;width=46&amp;show_faces=true&amp;action=like&amp;colorscheme=light&amp;font=arial&amp;height=60" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:46px; height:60px;" allowTransparency="true"></iframe>
  </li>
  <li class="item">
    <script type="text/javascript">
    /* <![CDATA[ */
        (function() {
            var s = document.createElement('script'), t = document.getElementsByTagName('script')[0];
            s.type = 'text/javascript';
            s.async = true;
            s.src = 'http://api.flattr.com/js/0.6/load.js?mode=auto';
            t.parentNode.insertBefore(s, t);
        })();
    /* ]]> */
    </script>
    <a class="FlattrButton" style="display:none;" href="<%= HOST %>"></a>
    <noscript><a href="http://flattr.com/thing/51992/TV-Episode-Calendar" target="_blank">
    <img src="http://api.flattr.com/button/flattr-badge-large.png" alt="Flattr this" title="Flattr this" border="0" /></a></noscript>
  </li>
  <li class="item">
    <form action="https://www.paypal.com/cgi-bin/webscr" method="post">
      <input type="hidden" name="cmd" value="_s-xclick">
      <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7----- ">
      <input id="paypal" type="image" src="<%= image_path("icons/paypal.png") %>" border="0" name="submit" alt="Donate">
    </form>
  </li>
</ul>